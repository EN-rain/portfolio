{
  "extends": ["next/core-web-vitals", "next/typescript", "next"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "react/prop-types": "off",
    "no-unused-vars": "off",
    "prefer-const": "off", // Disable prefer-const rule
    "@typescript-eslint/no-unused-vars": "off", // Disable TypeScript unused vars rule
    "no-var": "off", // Disable no-var rule
    "react-hooks/exhaustive-deps": "off", // Disable exhaustive-deps rule
    "@next/next/no-img-element": "off" // Disable no-img-element rule
  }
}
